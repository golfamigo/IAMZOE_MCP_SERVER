{
  "name": "iamzoe-mcp-server",
  "version": "1.0.0",
  "description": "MCP Server for IAMZOE",
  "main": "dist/index.js",
  "bin": {
    "iamzoe-mcp-server": "./dist/bin/cli.js"
  },
  "scripts": {
    "generate-tools": "ts-node src/scripts/generateToolDefs.ts",
    "generate-docs": "ts-node src/scripts/generateToolDocs.ts",
    "prebuild": "npm run generate-tools && npm run generate-docs",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:mcp": "cross-env START_MODE=mcp node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:mcp": "cross-env START_MODE=mcp ts-node src/index.ts",
    "test": "jest"
  },
  "keywords": [
    "MCP",
    "Neo4j",
    "API",
    "n8n"
  ],
  "author": "Your Name",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/golfamigo/IAMZOE_MCP_SERVER.git"
  },
  "bugs": {
    "url": "https://github.com/golfamigo/IAMZOE_MCP_SERVER/issues"
  },
  "homepage": "https://github.com/golfamigo/IAMZOE_MCP_SERVER#readme"
}